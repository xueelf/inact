{
  "name": "inact",
  "version": "0.0.2",
  "description": "Transpile the JSX to HTML strings",
  "exports": {
    "./jsx-runtime": {
      "types": "./types/jsx.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./types/jsx.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "packageManager": "bun@1.3.0",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "build": "bun ./scripts/build/index.ts",
    "format": "prettier **/*.ts --write",
    "lint": "eslint **/*.ts",
    "test": "bun test",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xueelf/inact.git"
  },
  "keywords": [
    "html",
    "jsx",
    "react"
  ],
  "author": "Yuki <admin@yuki.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xueelf/inact/issues"
  },
  "homepage": "https://github.com/xueelf/inact#readme",
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@eslint/js": "latest",
    "eslint": "latest",
    "husky": "latest",
    "prettier": "latest",
    "typescript-eslint": "latest"
  },
  "peerDependencies": {
    "typescript": "latest"
  },
  "devEngines": {
    "runtime": {
      "name": "bun",
      "onFail": "error"
    },
    "packageManager": {
      "name": "bun",
      "onFail": "error"
    }
  }
}
